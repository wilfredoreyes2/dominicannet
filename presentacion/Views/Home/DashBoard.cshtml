@{
    ViewBag.Title = "DashBoard";
    Layout = "~/Views/Shared/_BarraNavAdmin.cshtml";
}



<main style="width: 100%; height: 82vh;" class="d-flex justify-content-center align-items-center">
    <script src="https://kit.fontawesome.com/0d9a9d95d8.js" crossorigin="anonymous"></script>

    <div class="d-flex justify-content-center shadow p-3 mb-5 bg-body rounded">

        <div style="margin: 20px" class="d-flex justify-content-center flex-column shadow-none p-3 mb-5 bg-light rounded">
            <div style="margin: 20px" class="d-flex justify-content-between align-items-center">
                <span class="fw-bold">proyectos</span>

                <svg xmlns="http://www.w3.org/2000/svg" height="50" fill="currentColor" class="bi bi-grid" viewBox="0 0 16 16">
                    <path d="M1 2.5A1.5 1.5 0 0 1 2.5 1h3A1.5 1.5 0 0 1 7 2.5v3A1.5 1.5 0 0 1 5.5 7h-3A1.5 1.5 0 0 1 1 5.5v-3zM2.5 2a.5.5 0 0 0-.5.5v3a.5.5 0 0 0 .5.5h3a.5.5 0 0 0 .5-.5v-3a.5.5 0 0 0-.5-.5h-3zm6.5.5A1.5 1.5 0 0 1 10.5 1h3A1.5 1.5 0 0 1 15 2.5v3A1.5 1.5 0 0 1 13.5 7h-3A1.5 1.5 0 0 1 9 5.5v-3zm1.5-.5a.5.5 0 0 0-.5.5v3a.5.5 0 0 0 .5.5h3a.5.5 0 0 0 .5-.5v-3a.5.5 0 0 0-.5-.5h-3zM1 10.5A1.5 1.5 0 0 1 2.5 9h3A1.5 1.5 0 0 1 7 10.5v3A1.5 1.5 0 0 1 5.5 15h-3A1.5 1.5 0 0 1 1 13.5v-3zm1.5-.5a.5.5 0 0 0-.5.5v3a.5.5 0 0 0 .5.5h3a.5.5 0 0 0 .5-.5v-3a.5.5 0 0 0-.5-.5h-3zm6.5.5A1.5 1.5 0 0 1 10.5 9h3a1.5 1.5 0 0 1 1.5 1.5v3a1.5 1.5 0 0 1-1.5 1.5h-3A1.5 1.5 0 0 1 9 13.5v-3zm1.5-.5a.5.5 0 0 0-.5.5v3a.5.5 0 0 0 .5.5h3a.5.5 0 0 0 .5-.5v-3a.5.5 0 0 0-.5-.5h-3z" />
                </svg>

            </div>
            <div class="d-flex justify-content-center flex-row " style="margin: 10px">
                @Html.ActionLink("Listar", "Tablaproyectos", "Home", new { @class = "btn btn-primary", @style = "width: 100px; margin: 5px" })
                @Html.ActionLink("Agregar", "Agregarproyecto", "Home", new { @class = "btn btn-primary", @style = "width: 100px; margin: 5px" })
            </div>
        </div>

        <div style="margin: 20px" class="d-flex justify-content-center flex-column shadow-none p-3 mb-5 bg-light rounded">
            <div style="margin: 20px" class="d-flex justify-content-between align-items-center">
                <span class="fw-bold">Empresas</span>


                <svg xmlns="http://www.w3.org/2000/svg" height="50" fill="currentColor" class="bi bi-pc-display" viewBox="0 0 16 16">
                    <path d="M8 1a1 1 0 0 1 1-1h6a1 1 0 0 1 1 1v14a1 1 0 0 1-1 1H9a1 1 0 0 1-1-1V1Zm1 13.5a.5.5 0 1 0 1 0 .5.5 0 0 0-1 0Zm2 0a.5.5 0 1 0 1 0 .5.5 0 0 0-1 0ZM9.5 1a.5.5 0 0 0 0 1h5a.5.5 0 0 0 0-1h-5ZM9 3.5a.5.5 0 0 0 .5.5h5a.5.5 0 0 0 0-1h-5a.5.5 0 0 0-.5.5ZM1.5 2A1.5 1.5 0 0 0 0 3.5v7A1.5 1.5 0 0 0 1.5 12H6v2h-.5a.5.5 0 0 0 0 1H7v-4H1.5a.5.5 0 0 1-.5-.5v-7a.5.5 0 0 1 .5-.5H7V2H1.5Z" />
                </svg>

            </div>
            <div class="d-flex justify-content-center flex-row " style="margin: 10px">
                @Html.ActionLink("Listar", "Tablaempresas", "Home", new { @class = "btn btn-primary", @style = "width: 100px; margin: 5px" })
                @Html.ActionLink("Agregar", "Agregarempresas", "Home", new { @class = "btn btn-primary", @style = "width: 100px; margin: 5px" })
            </div>
        </div>

        <div style="margin: 20px" class="d-flex justify-content-center flex-column shadow-none p-3 mb-5 bg-light rounded">
            <div style="margin: 20px" class="d-flex justify-content-between align-items-center">
                <span class="fw-bold">Usuarios</span>

                <svg xmlns="http://www.w3.org/2000/svg" height="50" fill="currentColor" class="bi bi-person-square" viewBox="0 0 16 16">
                    <path d="M11 6a3 3 0 1 1-6 0 3 3 0 0 1 6 0z" />
                    <path d="M2 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2zm12 1a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1v-1c0-1-1-4-6-4s-6 3-6 4v1a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h12z" />
                </svg>


            </div>
            <div class="d-flex justify-content-center flex-row " style="margin: 10px">
                @Html.ActionLink("Listar", "Tablausuarios", "Home", new { @class = "btn btn-primary", @style = "width: 100px; margin: 5px" })
                @Html.ActionLink("Agregar", "Agregarusuarios", "Home", new { @class = "btn btn-primary", @style = "width: 100px; margin: 5px" })
            </div>
        </div>

        <div style="margin: 20px" class="d-flex justify-content-center flex-column shadow-none p-3 mb-5 bg-light rounded">
            <div style="margin: 20px" class="d-flex justify-content-between align-items-center">
                <span class="fw-bold">Reportes</span>

                <svg xmlns="http://www.w3.org/2000/svg" height="50" fill="currentColor" class="bi bi-exclamation-circle" viewBox="0 0 16 16" style="position:relative; right: -15px;">
                    <path d="M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14zm0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16z" />
                    <path d="M7.002 11a1 1 0 1 1 2 0 1 1 0 0 1-2 0zM7.1 4.995a.905.905 0 1 1 1.8 0l-.35 3.507a.552.552 0 0 1-1.1 0L7.1 4.995z" />
                </svg>


            </div>
            <div class="d-flex justify-content-center flex-row " style="margin: 10px">
                @Html.ActionLink("Listar", "TablaReporteView", "Home", new { @class = "btn btn-primary", @style = "width: 100px; margin: 5px" })
            </div>
        </div>
        <div style="margin: 20px" class="d-flex justify-content-center flex-column shadow-none p-3 mb-5 bg-light rounded">
            <div style="margin: 20px" class="d-flex justify-content-between align-items-center">
                <span class="fw-bold">vacantes</span>

                <svg xmlns="http://www.w3.org/2000/svg" height="50" fill="currentColor" class="bi bi-person-square" viewBox="0 0 16 16">
                    <path d="M11 6a3 3 0 1 1-6 0 3 3 0 0 1 6 0z" />
                    <path d="M2 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2zm12 1a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1v-1c0-1-1-4-6-4s-6 3-6 4v1a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h12z" />
                </svg>


            </div>
            <div class="d-flex justify-content-center flex-row " style="margin: 10px">
                @Html.ActionLink("Listar", "Tablavacantes_empresas", "Home", new { @class = "btn btn-primary", @style = "width: 100px; margin: 5px" })
                @Html.ActionLink("Agregar", "Agregarvacantes_empresas", "Home", new { @class = "btn btn-primary", @style = "width: 100px; margin: 5px" })
            </div>
        </div>

    </div>
    @*@Html.ActionLink("Categorias del los productos", "TablaCategoria", "Home", new { @class = "btn btn-primary", @style = "margin: 5px" })
        @Html.ActionLink("Listado de productos", "TablaProductos", "Home", new { @class = "btn btn-primary", @style = "margin: 5px" })
        @Html.ActionLink("Listados de usuarios", "TablaUsuario", "Home", new { @class = "btn btn-primary", @style = "margin: 5px" })*@
    @*<button style="margin: 5px" type="button" class="btn btn-primary">Categorias del los productos</button>
        <button style="margin: 5px" type="button" class="btn btn-primary">Listado de productos</button>
        <button style="margin: 5px" type="button" class="btn btn-primary">Listados de usuarios</button>*@

</main>
